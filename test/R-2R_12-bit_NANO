unsigned long count;

void setup() {
  pinMode(2, OUTPUT);
  pinMode(3, OUTPUT);
  pinMode(4, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(6, OUTPUT);
  pinMode(7, OUTPUT);
  pinMode(8, OUTPUT);
  pinMode(9, OUTPUT);
  pinMode(10, OUTPUT);
  pinMode(11, OUTPUT);
  pinMode(12, OUTPUT);
  pinMode(13, OUTPUT);
}

void loop() {
  if (count%2 > 0){
    digitalWrite(2, HIGH);
  }
  else {
    digitalWrite(2, LOW);
  }
  if (count%4 > 1){
    digitalWrite(3, HIGH);
  }
  else {
    digitalWrite(3, LOW);
  }
  if (count%8 > 3){
    digitalWrite(4, HIGH);
  }
  else {
    digitalWrite(4, LOW);
  }
  if (count%16 > 7){
    digitalWrite(5, HIGH);
  }
  else {
    digitalWrite(5, LOW);
  }
  if (count%32 > 15){
    digitalWrite(6, HIGH);
  }
  else {
    digitalWrite(6, LOW);
  }
  if (count%64 > 31){
    digitalWrite(7, HIGH);
  }
  else {
    digitalWrite(7, LOW);
  }
  if (count%128 > 63){
    digitalWrite(8, HIGH);
  }
  else {
    digitalWrite(8, LOW);
  }
  if (count%256 > 117){
    digitalWrite(9, HIGH);
  }
  else {
    digitalWrite(9, LOW);
  }
  if (count%512 > 255){
    digitalWrite(10, HIGH);
  }
  else {
    digitalWrite(10, LOW);
  }
  if (count%1024 > 511){
    digitalWrite(11, HIGH);
  }
  else {
    digitalWrite(11, LOW);
  }
  if (count%2048 > 1023){
    digitalWrite(12, HIGH);
  }
  else {
    digitalWrite(12, LOW);
  }
  if (count%4096 > 2047){
    digitalWrite(13, HIGH);
  }
  else {
    digitalWrite(13, LOW);
  }
  
  count++;
  delay(50);
}
